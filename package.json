{
  "name": "@neo4j/gh-changelog",
  "version": "1.0.0",
  "description": "Generate a changelog from PR:s in a Github repo",
  "bin": {
    "gh-changelog": "lib/generate.js"
  },
  "scripts": {
    "test": "jest",
    "generate": "babel-node src/generate.js",
    "precommit": "lint-staged",
    "build": "babel src -d lib"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.{js,jsx,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8"
  },
  "author": "Neo4j Inc.",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^23.4.2",
    "babel-plugin-shebang": "^0.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "husky": "^0.14.3",
    "jest": "^23.4.2",
    "lint-staged": "^7.2.0",
    "prettier": "^1.14.0",
    "yarn": "^1.9.4"
  },
  "dependencies": {
    "@octokit/rest": "^15.9.5",
    "babel-runtime": "^6.26.0",
    "semver-compare": "^1.0.0",
    "semver-sort": "^0.0.4"
  }
}
